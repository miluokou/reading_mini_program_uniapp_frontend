
.container {
	padding: 20rpx 0 120rpx;
	box-sizing: border-box;
	overflow-x: hidden;
}
.header {
	padding: 80rpx 90rpx 60rpx 90rpx;
	box-sizing: border-box;
}
.title {
	font-size: 34rpx;
	color: #333;
	font-weight: 500;
}
.sub-title {
	font-size: 24rpx;
	color: #7a7a7a;
	padding-top: 18rpx;
}
.tui-richtext {
	width: 100%;

	padding-right: 30rpx;

	box-sizing: border-box;
}
.tui-title {
	padding: 60rpx 30rpx 20rpx 30rpx;
	font-size: 26rpx;
	color: #7a7a7a;
}
.tui-code {
	width: 100%;
	padding: 16rpx;
	box-sizing: border-box;
}
.tui-scroll-view {
	width: 100%;
	border: 1rpx solid #19BE6B;
	padding: 10rpx;
	height: 240rpx;
	font-size: 24rpx;
	box-sizing: border-box;
}

